							<tr class="ia__cosmos__tr">
								<td class="ia__cosmos__td-field">{{= _(val, 'cosmos_fields') }}</td>
								<td class="ia__cosmos__td-val">{{= it.data.cosmic[val] }}</td>
							</tr>	
						{{~}}
					</table>
				</div>
			</div>
					</details>
				</article>
			</div>

			<div>
				<article>
					<details class="ia__cosmos__physical__marker">
						<summary class="ia__cosmos__physical__expand">{{=  _('Physical characteristics', 'cosmos_fields') }}</summary>
						<div class="ia__cosmos__physical-mobile">
				
				<div class="ia__cosmos__physical-left">

					<table style="width:100%">
					{{? it.data.cosmic['mass'] != 0 }}
					<tr class="ia__cosmos__tr">
						<td class="ia__cosmos__td-field"> {{= _('mass', 'cosmos_fields') }} </td>
						<td class="ia__cosmos__td-val"> {{= it.data.cosmic['massValue'] }}x10<sup>{{= it.data.cosmic['massExponent'] }}</sup> kg </td>
					</tr>
					{{?}}
					{{? it.data.cosmic['volume'] != 0 }}
					<tr class="ia__cosmos__tr">
						<td class="ia__cosmos__td-field"> {{= _('volume', 'cosmos_fields') }} </td>
						<td class="ia__cosmos__td-val"> {{= it.data.cosmic['volValue'] }}x10<sup>{{= it.data.cosmic['volExponent']}}</sup> km<sup>3</sup> </td>
					</tr>
					{{?}}
					{{? it.data.cosmic['density'] != 0 }}
					<tr class="ia__cosmos__tr">
						<td class="ia__cosmos__td-field"> {{= _('density', 'cosmos_fields') }} </td>
						<td class="ia__cosmos__td-val"> {{= it.data.cosmic['density'] }} g.cm<sup>3</sup> </td>
					</tr>
					{{?}}
					{{? it.data.cosmic['gravity'] != 0 }}
					<tr class="ia__cosmos__tr">
						<td class="ia__cosmos__td-field"> {{= _('gravity', 'cosmos_fields') }} </td>
						<td class="ia__cosmos__td-val"> {{= it.data.cosmic['gravity'] }} m.s<sup>-2</sup> </td>
					</tr class="ia__cosmos__tr">
					{{?}}
					{{? it.data.cosmic['ezcape'] != 0 }}
					<tr class="ia__cosmos__tr">
						<td class="ia__cosmos__td-field"> {{= _('ezcape', 'cosmos_fields') }} </td>
						<td class="ia__cosmos__td-val"> {{= it.data.cosmic['ezcape'] }} m.s<sup>-1</sup> </td>
					</tr>
					{{?}}
					</table>
				</div>

				
				<div class="ia__cosmos__physical-right">
					
					<table style="width:100%">
					{{? it.data.cosmic['meanRadius'] != 0 }}
					<tr class="ia__cosmos__tr">
						<td class="ia__cosmos__td-field"> {{= _('meanRadius', 'cosmos_fields') }} </td>
						<td class="ia__cosmos__td-val"> {{= it.data.cosmic['meanRadius'] }} km </td>
					</tr>
					{{?}}
					{{? it.data.cosmic['equaRadius'] != 0 }}
					<tr class="ia__cosmos__tr">
						<td class="ia__cosmos__td-field"> {{= _('equaRadius', 'cosmos_fields') }} </td>
						<td class="ia__cosmos__td-val"> {{= it.data.cosmic['equaRadius'] }} km </td>
					</tr>
					{{?}}
					{{? it.data.cosmic['polarRadius'] != 0 }}
					<tr class="ia__cosmos__tr">
						<td class="ia__cosmos__td-field"> {{= _('polarRadius', 'cosmos_fields') }} </td>
						<td class="ia__cosmos__td-val"> {{= it.data.cosmic['polarRadius'] }} </td>
					</tr>
					{{?}}
					{{? it.data.cosmic['flattening'] != 0 }}
					<tr class="ia__cosmos__tr">
						<td class="ia__cosmos__td-field"> {{= _('flattening', 'cosmos_fields') }} </td>
						<td class="ia__cosmos__td-val"> {{= it.data.cosmic['flattening'] }} </td>
					</tr>
					{{?}}
					{{? it.data.cosmic['sideralRotation'] != 0 }}
					<tr class="ia__cosmos__tr">
						<td class="ia__cosmos__td-field"> {{= _('sideralRotation', 'cosmos_fields') }} </td>
						<td class="ia__cosmos__td-val"> {{= it.data.cosmic['sideralRotation'] }} h </td>
					</tr>
					{{?}}
					</table>
					
				</div>

			</div>
						
					</details>
				</article>
			</div>
			
	{{?}}
	
	
</div>

